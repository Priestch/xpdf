{
  "$schema": "https://schema.tauri.app/config/2.0",
  "productName": "PDF-X Viewer",
  "version": "0.1.0",
  "identifier": "com.pdf-x.viewer",
  "build": {
    "devUrl": "http://localhost:5173",
    "frontendDist": "ui/dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "PDF-X Viewer",
        "width": 1200,
        "height": 800,
        "resizable": true,
        "fullscreen": false,
        "minWidth": 800,
        "minHeight": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all"
  }
}
